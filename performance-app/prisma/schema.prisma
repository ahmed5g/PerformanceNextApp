generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model store {
  id       String   @id @default(cuid())
  name     String   @unique
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}
